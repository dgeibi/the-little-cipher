{
  "name": "the-little-cipher",
  "version": "1.0.0",
  "product_name": "The Little Cipher",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier-eslint --write '**/*.{js,css}'",
    "precommit": "lint-staged",
    "dev": "nodemon --watch src/server src/server/index.js --exec \"cross-env NODE_ENV=development node -r babel-register\"",
    "prod": "npm run build && npm start",
    "postinstall": "npm run build",
    "start": "node dist/server/index.js",
    "build": "npm run clean && run-p build:*",
    "clean": "rimraf dist",
    "build:static": "webpack --config config/webpack.client.js --env.production",
    "build:server": "webpack --config config/webpack.server.js",
    "analyze": "webpack --config config/webpack.analyzer.js"
  },
  "author": "dgeibi",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 90
  },
  "lint-staged": {
    "*.{css,js}": [
      "git-exec-and-restage prettier-eslint --write --"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-dgeibi": "^3.0.6",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "git-exec-and-restage": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nodemon": "^1.12.1",
    "redbox-react": "^1.5.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "antd": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-macros": "^1.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codegen.macro": "^1.0.0",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "dva": "^2.1.0",
    "dva-loading": "^1.0.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "multer": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "rc-table": "^6.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fns": "^1.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.0",
    "unfetch": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/the-little-cipher.git"
  },
  "bugs": {
    "url": "https://github.com/dgeibi/the-little-cipher/issues"
  },
  "homepage": "https://github.com/dgeibi/the-little-cipher#readme",
  "description": "实现一些加密算法"
}
