{
  "name": "the-little-cipher",
  "version": "1.0.0",
  "product_name": "The Little Cipher",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,css}\" --ignore-path .gitignore",
    "precommit": "lint-staged",
    "dev": "nodemon --watch src/server src/server/dev.js --exec \"node -r @babel/register\"",
    "prod": "npm run build && npm start",
    "start": "node dist/server/index.js",
    "build": "npm run clean && run-p build:*",
    "heroku-postbuild": "npm run build",
    "clean": "rimraf dist",
    "build:static": "webpack --config config/webpack.client.prod.js",
    "build:server": "webpack --config config/webpack.server.js",
    "analyze": "webpack --config config/webpack.analyzer.js"
  },
  "author": "dgeibi",
  "license": "MIT",
  "lint-staged": {
    "*.{css,js}": [
      "git-exec-and-restage prettier --write --"
    ]
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.4",
    "eslint": "^4.16.0",
    "eslint-config-dgeibi": "^4.1.0",
    "git-exec-and-restage": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.12.5",
    "prettier": "^1.9.2",
    "redbox-react": "^1.5.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-react": "7.0.0-beta.40",
    "@babel/preset-stage-3": "7.0.0-beta.40",
    "antd": "^3.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "compression": "^1.7.1",
    "css-loader": "^0.28.8",
    "dva": "^2.1.0",
    "dva-hot": "^0.2.0",
    "dva-loading": "^1.0.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "multer": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "rc-table": "^6.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fns": "^1.3.0",
    "react-helmet": "^5.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "simple-prerender-webpack-plugin": "^1.0.0",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "unfetch": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/the-little-cipher.git"
  },
  "bugs": {
    "url": "https://github.com/dgeibi/the-little-cipher/issues"
  },
  "homepage": "https://github.com/dgeibi/the-little-cipher#readme",
  "description": "实现一些加密算法"
}
