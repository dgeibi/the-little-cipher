{
  "presets": [
    [
      "babel-preset-env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "current"
        }
      }
    ],
    "babel-preset-stage-2",
    "babel-preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    [
      "babel-plugin-transform-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
