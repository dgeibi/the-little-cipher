{
  "env": {
    "jest": true
  },
  "extends": "dgeibi/react",
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "global-require": "off",
    "no-mixed-operators": "off",
    "react/prop-types": "off",
    "no-underscore-dangle": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.base.js"
      }
    }
  }
}
